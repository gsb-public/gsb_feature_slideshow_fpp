{
  "name": "drupal/gsb_feature_slideshow_fpp",
  "description": "GSB Feature Slideshow FPP",
  "license": "Apache-2.0",
  "repositories": [
    {
        "type": "package",
        "package": {
            "name": "woothemes/flexslider",
            "version": "master",
            "type": "drupal-gsb-library", 
            "dist": {
                "url": "https://github.com/woothemes/FlexSlider/archive/version/2.1.zip",
                "type": "zip"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "kenwheeler/slick",
            "version": "master",
            "type": "drupal-gsb-library", 
            "dist": {
                "url": "https://github.com/kenwheeler/slick/archive/1.6.0.zip",
                "type": "zip"
            }
        }
    }
  ],  
  "require": {
    "php": ">=5.3.0",
    "woothemes/flexslider": "master",
    "kenwheeler/slick": "master"
  },
  "extra": {
    "installer-paths": {
      "web/profiles/gsb_public/libraries/{$name}/": ["type:drupal-gsb-library"]
    }
  } 
}
